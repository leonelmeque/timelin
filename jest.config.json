{
  "preset": "react-native",
  "setupFilesAfterEnv": [
    "@babel/preset-typescript",
    "@testing-library/jest-native/extend-expect"
  ],
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/__mocks__/fileMock.js"
  },
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 80,
      "lines": 80
    }
  }
}
